  "AWSTemplateFormatVersion":"2010-09-09",
  "Description":"Cloudformation template for supply dynamodb tables",
  "Resources":{
    "ExternalsBehance":{
      "Type":"AWS::DynamoDB::Table",
      "Properties":{
        "AttributeDefinitions":[
          {
            "AttributeName":"userId",
            "AttributeType":"N"
          }
        ],
        "KeySchema":[
          {
            "AttributeName":"userId",
            "KeyType":"HASH"
          }
        ],
